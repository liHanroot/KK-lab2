{  "grammar": {    "-name": "G0",    "terminalsymbols": [        {          "-name": "a",          "-spell": "a"        },        {          "-name": "+",          "-spell": "+"        },        {          "-name": "*",          "-spell": "*"        },        {          "-name": "(",          "-spell": "("        },        {          "-name": ")",          "-spell": ")"        }      ],    "nonterminalsymbols":    [		"P",        "A",        "B"    ],    "productions": [        {          "lhs": "P",          "rhs": [              {                "-type": "nonterm",                "-name": "P"              },              {                "-type": "term",                "-name": ""              },              {                "-type": "nonterm",                "-name": "A"              }            ]        },		{          "lhs": "P",          "rhs": [              {                "-type": "nonterm",                "-name": "B"              }            ]        }    ],    "startsymbol": "P"  }}